# Example environment variables for all AI providers
# Replace values with your actual API keys before use

AI_GATEWAY_API_KEY=your-vercel-ai-gateway-api-key
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
GOOGLE_API_KEY=your-google-api-key
ZHIPU_API_KEY=your-zhipu-api-key
ZHIPU_BASE_URL=https://api.z.ai/api/paas/v4
ZHIPU_MODEL=glm-4.7

# Model selection for each provider
OPENAI_MODEL=gpt-4-turbo
ANTHROPIC_MODEL=claude-3-sonnet
GOOGLE_MODEL=gemini-pro

# MAF (Microsoft Agent Framework) Backend
# Set this to enable maf-standalone integration
NEXT_PUBLIC_MAF_URL=http://localhost:5555

# Clerk Authentication
# Get your keys from https://dashboard.clerk.com/
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_YOUR_PUBLISHABLE_KEY
CLERK_SECRET_KEY=sk_test_YOUR_SECRET_KEY

# Neon Database (PostgreSQL)
# Get your connection string from https://console.neon.tech/
DATABASE_URL=postgresql://user:password@ep-xxx.region.aws.neon.tech/dbname?sslmode=require

# Clerk Routes (optional - customize sign-in/sign-up URLs)
NEXT_PUBLIC_CLERK_SIGN_IN_URL=/sign-in
NEXT_PUBLIC_CLERK_SIGN_UP_URL=/sign-up
NEXT_PUBLIC_CLERK_SIGN_IN_FALLBACK_REDIRECT_URL=/
NEXT_PUBLIC_CLERK_SIGN_UP_FALLBACK_REDIRECT_URL=/

# Turbopack Optimization Environment Variables
# Enable Turbopack for development and production builds
TURBOPACK=1

# Turbopack cache directory (optional - defaults to .next/turbo)
# TURBOPACK_CACHE_DIR=.next/turbo

# Increase Turbopack memory limit for large projects (in MB)
# Default is 2048, increase if you encounter out-of-memory errors
TURBOPACK_MEMORY_LIMIT=4096

# Enable Turbopack's experimental features
# TURBOPACK_EXPERIMENTAL=1

# Turbopack parallel processing (default: true)
# TURBOPACK_PARALLEL=true

# Turbopack verbose logging for debugging
# TURBOPACK_VERBOSE=0
